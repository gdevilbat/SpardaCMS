{
    "name": "gdevilbat/spardacms",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "composer/installers": "^1.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "replace": {
        "gdevilbat/sparda-cms-core-module": "*",
        "gdevilbat/sparda-cms-account-module": "*",
        "gdevilbat/sparda-cms-role-module": "*",
        "gdevilbat/sparda-cms-user-module": "*",
        "gdevilbat/sparda-cms-taxonomy-module": "*",
        "gdevilbat/sparda-cms-post-module": "*",
        "gdevilbat/sparda-cms-page-module": "*",
        "gdevilbat/sparda-cms-appearance-module": "*",
        "gdevilbat/sparda-cms-blog-module": "*",
        "gdevilbat/sparda-cms-ecommerce-module": "*"
    },
    "repositories": [
        {
            "name": "gdevilbat/sparda-cms-core-module",
            "type": "vcs",
            "url": "https://gitlab.com/maldinuribrahim/spardacms-core"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "module-dir": "vendor/gdevilbat",
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ],
            "recurse": true,
            "replace": true,
            "merge-extra": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Gdevilbat\\SpardaCMS\\Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
